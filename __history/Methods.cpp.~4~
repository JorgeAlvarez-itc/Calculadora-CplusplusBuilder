#include <cmath>

#include "Methods.h"


double suma(double a, double b)
{
	return a+b;
}

double resta(double a, double b)
{
	return a-b;
}

double mult(double a, double b)
{
	return a*b;
}

double div(double a, double b)
{
	return a/b;
}

void equals(double a, double b, int op)
{
	switch(op)
	{
		case 1: break;
		case 2: break;
		case 3: break;
		case 4: break;
	}
	//return a+b;
}

double sqRoot(double a)
{
	return sqrt(a);
}

double percentage(double a)
{
	return a * 0.01;
}

bool checkDot(String s)
{
	int a = s.find(".");
	if (a = s.npos) {
		 return true;
	}
    else return false;
}
